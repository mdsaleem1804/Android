        <activity android:name="com.example.ag_and_014_a_speechbasedsms.SpeechBasedSms" >
        </activity>
        <activity android:name="com.example.ag_and_014_a_speechbasedsms.ContactsManager" >
        </activity>
    </application>

</manifest>