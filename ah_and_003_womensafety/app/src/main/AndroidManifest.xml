<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hellotamila.ah_and_003_womensafety">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_CALL_LOG"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/womensafety"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/womensafety"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hellotamila.ah_and_003_womensafety.HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hellotamila.ah_and_003_womensafety.Level1"
            />
        <activity android:name="com.hellotamila.ah_and_003_womensafety.AndroidGPSTrackingActivity" />
        <activity android:name="com.hellotamila.ah_and_003_womensafety.Level2"></activity>
        <activity android:name=".Webform"></activity>
    </application>

</manifest>