<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hellotamila.ah_006_tourist">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/mns"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddBus"></activity>
        <activity android:name=".AddHotel"></activity>
        <activity android:name=".AddPlaces"></activity>
        <activity android:name=".AddRoute"></activity>
        <activity android:name=".AdminBusEdit"></activity>
        <activity android:name=".AdminMainPage"></activity>
        <activity android:name=".AdminLoginPage"></activity>
        <activity android:name=".GetBudget"></activity>
        <activity android:name=".ListBus"></activity>
        <activity android:name=".ListPlaces"></activity>
        <activity android:name=".ListHotel"></activity>
        <activity android:name=".UserViewBus"></activity>
        <activity android:name=".UserMainPage"></activity>
        <activity android:name=".MapsActivity"></activity>
        <activity android:name=".Webform"></activity>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>